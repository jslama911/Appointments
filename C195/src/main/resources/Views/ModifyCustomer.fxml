        <Button fx:id="saveButton" layoutX="374.0" layoutY="360.0" mnemonicParsing="false" onAction="#saveButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Modify" />
        <Button fx:id="clearButton" layoutX="489.0" layoutY="360.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
      <Label layoutX="253.0" layoutY="14.0" text="Modify Customer" />
