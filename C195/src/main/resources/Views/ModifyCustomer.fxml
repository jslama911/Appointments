<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ModifyCustomerController">
    <children>
        <Label layoutX="41.0" layoutY="54.0" prefHeight="17.0" prefWidth="71.0" text="Customer ID" />
        <TextField fx:id="modifyCustomerIDTextField" disable="true" editable="false" layoutX="124.0" layoutY="49.0" prefHeight="25.0" prefWidth="197.0" />
        <Label layoutX="41.0" layoutY="96.0" prefHeight="17.0" prefWidth="71.0" text="Country" />
        <ComboBox fx:id="modifyCustomerCountryComboBox" layoutX="124.0" layoutY="91.0" prefHeight="25.0" prefWidth="196.0" />
        <Label layoutX="41.0" layoutY="134.0" prefHeight="17.0" prefWidth="71.0" text="Division" />
        <ComboBox fx:id="modifyCustomerDivisionComboBox" layoutX="124.0" layoutY="129.0" prefHeight="25.0" prefWidth="196.0" />
        <Label layoutX="41.0" layoutY="179.0" prefHeight="17.0" prefWidth="71.0" text="Name" />
        <TextField fx:id="modifyCustomerNameTextField" layoutX="123.0" layoutY="174.0" prefHeight="25.0" prefWidth="197.0" />
        <Label layoutX="41.0" layoutY="220.0" prefHeight="17.0" prefWidth="71.0" text="Address" />
        <TextField fx:id="modifyCustomerAddressTextField" layoutX="123.0" layoutY="215.0" prefHeight="25.0" prefWidth="197.0" />
