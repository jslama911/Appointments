<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.AddContactController">
    <children>
        <Label layoutX="41.0" layoutY="109.0" prefHeight="17.0" prefWidth="71.0" text="Name" />
        <TextField fx:id="contactNameTextField" layoutX="123.0" layoutY="104.0" prefHeight="25.0" prefWidth="197.0" />
        <Label layoutX="41.0" layoutY="168.0" prefHeight="17.0" prefWidth="71.0" text="Email" />
        <TextField fx:id="contactEmailTextField" layoutX="123.0" layoutY="164.0" prefHeight="25.0" prefWidth="197.0" />
        <Button fx:id="saveButton" layoutX="374.0" layoutY="360.0" mnemonicParsing="false" onAction="#saveContactClicked" prefHeight="25.0" prefWidth="70.0" text="Add" />
        <Button fx:id="clearButton" layoutX="489.0" layoutY="360.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
        <Label layoutX="260.0" layoutY="14.0" text="Add Contact" />
    </children>

