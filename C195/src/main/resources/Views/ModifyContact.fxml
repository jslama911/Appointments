<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.ModifyContactController">
    <children>
        <Label layoutX="41.0" layoutY="54.0" prefHeight="17.0" prefWidth="71.0" text="Contact ID" />
        <TextField fx:id="modifyContactIDTextField" disable="true" editable="false" layoutX="124.0" layoutY="49.0" prefHeight="25.0" prefWidth="197.0" />
        <Label layoutX="41.0" layoutY="109.0" prefHeight="17.0" prefWidth="71.0" text="Name" />
        <TextField fx:id="modifyContactNameTextField" layoutX="123.0" layoutY="104.0" prefHeight="25.0" prefWidth="197.0" />
        <Label layoutX="41.0" layoutY="168.0" prefHeight="17.0" prefWidth="71.0" text="Email" />
        <TextField fx:id="modifyContactEmailTextField" layoutX="123.0" layoutY="164.0" prefHeight="25.0" prefWidth="197.0" />
        <Button fx:id="modifySaveButton" layoutX="374.0" layoutY="360.0" mnemonicParsing="false" prefHeight="25.0" prefWidth="70.0" onAction="#saveButtonClicked" text="Add" />
        <Button fx:id="modifyExitButton" layoutX="489.0" layoutY="360.0" mnemonicParsing="false" onAction="#exitButtonClicked" prefHeight="25.0" prefWidth="70.0" text="Cancel" />
        <Label layoutX="260.0" layoutY="14.0" text="Add Contact" />
    </children>

</AnchorPane>
